{
  "name": "xml-writer-ts",
  "version": "0.0.5",
  "description": "A typescript XML writer",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint \"src/**/*.ts\"",
    "test:unit": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "build": "rollup -c",
    "check-types": "tsc --noemit",
    "fsm": "xstate typegen src/fsm.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guid75/xml-writer.git"
  },
  "keywords": [
    "XML"
  ],
  "author": "Guillaume Denry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Guid75/xml-writer/issues"
  },
  "homepage": "https://github.com/Guid75/xml-writer#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.33.1",
    "@xstate/cli": "^0.3.2",
    "dedent-js": "^1.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "xstate": "^4.33.2"
  }
}
